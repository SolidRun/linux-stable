			interrupts = <GIC_SPI 107 IRQ_TYPE_LEVEL_HIGH>;
			clocks = <&cpg CPG_MOD R9A09G011_CPERI_GRPA_PCLK>,
				 <&cpg CPG_MOD R9A09G011_TIM0_CLK>;
