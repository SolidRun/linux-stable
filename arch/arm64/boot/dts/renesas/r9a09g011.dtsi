			clocks = <&cpg CPG_MOD R9A09G011_PFC_PCLK>;
			power-domains = <&cpg>;
			resets = <&cpg R9A09G011_PFC_PRESETN>;
		};

		tim0: timer@a4000000 {
			compatible = "renesas,r9a09g011-tim-cnt",
