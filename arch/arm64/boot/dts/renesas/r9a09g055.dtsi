			compatible = "renesas,rzv2ma-dmac",
                                     "renesas,r9a09g055-dmac";
                    	reg = <0 0x82100000 0 0x800>;
                    	interrupts = <GIC_SPI 388 IRQ_TYPE_LEVEL_HIGH>,
