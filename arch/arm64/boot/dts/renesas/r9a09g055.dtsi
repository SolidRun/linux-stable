	                    clocks = <&cpg CPG_MOD R9A09G011_DMAA_ACLK>;
	                    clock-names = "dmaa_aclk";
	                    power-domains = <&cpg>;
	                    resets = <&cpg R9A09G011_DMAA_ARESETN>;
	                    #dma-cells = <1>;
	                    dma-channels = <16>;
