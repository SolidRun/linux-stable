			resets = <&cpg R9A07G043F_ADC_PRESETN>,
				 <&cpg R9A07G043F_ADC_ADRST_N>;
			reset-names = "presetn", "adrst-n";
			power-domains = <&cpg>;
			status = "disabled";

			#address-cells = <1>;
			#size-cells = <0>;

			channel@0 {
				reg = <0>;
